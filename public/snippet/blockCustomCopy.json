    "snippet": [
      "const matchGroup = input.block.content.match(/(第)(.*?)(条)/);",
      "let realTitle = input.block.hpath.match(/《.*?》/);",
      "if (!realTitle) {",
      "  realTitle = input.extra.title;",
      "}",
      "let result = \"\";",
      "if (matchGroup) {",
      "  result = `((${input.block.id} '${realTitle}${matchGroup[1]}${chineseToNum(",
      "    matchGroup[2],",
      "  )}${matchGroup[3]}'))`;",
      "} else {",
      "  result = `((${id} '${realTitle}${content.substring(0, 5)}'))`;",
      "}",
      "output = result;",
      "function chineseToNum(chnStr) {",
      "  let chnNumChar = {",
      "    零: 0,",
      "    一: 1,",
      "    二: 2,",
      "    两: 2,",
      "    三: 3,",
      "    四: 4,",
      "    五: 5,",
      "    六: 6,",
      "    七: 7,",
      "    八: 8,",
      "    九: 9,",
      "  };",
      "  let chnNameValue = {",
      "    十: { value: 10, secUnit: false },",
      "    百: { value: 100, secUnit: false },",
      "    千: { value: 1000, secUnit: false },",
      "    万: { value: 10000, secUnit: true },",
      "    亿: { value: 100000000, secUnit: true },",
      "  };",
      "  let rtn = 0;",
      "  let section = 0;",
      "  let number = 0;",
      "  let secUnit = false; //是否含有子单位",
      "  let str = chnStr.split(\"\");",
      "",
      "  for (let i = 0; i < str.length; i++) {",
      "    let num = chnNumChar[str[i]];",
      "    let unit = chnNameValue[str[i]] ? chnNameValue[str[i]].value : null;",
      "    secUnit = chnNameValue[str[i]] ? chnNameValue[str[i]].secUnit : null;",
      "    if (num) {",
      "      number = num;",
      "      if (i === str.length - 1) {",
      "        //处理个位",
      "        section += number;",
      "      }",
      "    } else if (unit) {",
      "      if (unit === 10 && number === 0) {",
      "        //十五变为一十五",
      "        number = 1;",
      "      }",
      "      if (secUnit) {",
      "        //处理万、亿等带子单位",
      "        section = (section + number) * unit;",
      "        rtn += section;",
      "        section = 0;",
      "      } else {",
      "        section += number * unit;",
      "      }",
      "      number = 0;",
      "    }",
      "  }",
      "  let result = rtn + section; //带子单位+万以下",
      "  return result;",
      "}",
      "",
      ""
    ],
    "name": "法条复制"
  }
]
